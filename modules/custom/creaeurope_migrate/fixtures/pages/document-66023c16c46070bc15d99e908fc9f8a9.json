f( $(this).find(\"city\").text() != '') {             dataStruct = dataStruct + '&lt;p class=\"hlcity\"&gt;' + $(this).find(\"city\").text()+ '&lt;/p&gt;' ;         };         if( $(this).find(\"tel\").text() != '') {             dataStruct = dataStruct + '&lt;p class=\"hltel\"&gt;Tel: ' + $(this).find(\"tel\").text()+ '&lt;/p&gt;' ;         };         if( $(this).find(\"fax\").text() != '') {             dataStruct = dataStruct + '&lt;div class=\"clearer\"&gt;&lt;/div&gt;&lt;p class=\"hlfax\"&gt;Fax: ' + $(this).find(\"fax\").text()+ '&lt;/p&gt;' ;         };         eCount = 0;         $(this).find('email address').each(function(){                 if( $(this).text() != '') {                     if( eCount == 0) {                         dataStruct = dataStruct + '&lt;div class=\"clearer\"&gt;&lt;/div&gt;&lt;p class=\"hlemail\"&gt;E-mail: &lt;/p&gt;';                         eCount++;                         };                 dataStruct = dataStruct + '&lt;p class=\"hleadd\"&gt;&lt;a href=\"mailto:' + $(this).text() + '\"&gt;' + $(this).text() + '&lt;/a&gt;&lt;/p&gt;';                 };         });         if( $(this).find(\"web\").text() != '') {             dataStruct = dataStruct + '&lt;p class=\"hlweb\"&gt;web: &lt;a href=\"http://' + $(this).find(\"web\").text() +'\"&gt;'+ $(this).find(\"web\").text() + '&lt;/a&gt;&lt;/p&gt;';         };             dataStruct = dataStruct + '&lt;div class=\"clear\"&gt; &lt;/div&gt;&lt;div class=\"totop\"&gt;&lt;a href=\"#top-page\"&gt;&lt;img src=\"/culture/images/arrow_top.png\" style=\"margin-left:400px\" /&gt;&lt;/a&gt;&lt;/div&gt;';         });      dataStruct = dataStruct + '&lt;/div&gt;';     });     $(\".pointsbox\").html(dataStruct);     $(\".countrybox\").fadeIn(1000); };  function hlshow(seeme){     $('.contryName').css('display','none');    document.getElementById(seeme).style.display=\"block\";     }     //      </script>             </div>"
